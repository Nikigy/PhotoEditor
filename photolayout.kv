<PhotoEditorManager>:
    PhotoEditorScreen:

<PhotoEditorScreen>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_y: None
        height: '48dp'
        Button:
            text: 'Load Image'
            on_release: root.load_image_popup()
        Button:
            text: 'Save Image'
            on_release: root.save_image()